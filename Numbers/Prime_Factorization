from math import sqrt
def prime(num):
	for i in range(2,round(sqrt(num)+1)):
		if num%i==0:
			return False
	else:
		return True

l1=[]
l2=[]

n=int(input("Enter a number for prime factorization : "))
for i in range(2,n):
	if n%i==0:
		if prime(i):
			l1.append(i)
		else:
			l2.append(i)

print("Prime Factors : ",l1)
print("Non-Prime Factors : ",l2)
